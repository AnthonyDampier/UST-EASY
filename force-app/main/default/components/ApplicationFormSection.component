<apex:component id="applicationFormSectionComponent">
    <apex:actionFunction name="setCreatingNewRelatedRecordAF{!group.item.Id}" action="{!groupNewRelatedRecord}" onComplete="pageLoadReRendered();" rerender="theSection">
        <apex:param name="p1" assignTo="{!group.newReferenceObjectSelectedWrapper}" value=""/>
        <apex:param name="p2" assignTo="{!group.newReferenceObjectSelectedWrapperName}" value=""/>
    </apex:actionFunction>
    <apex:attribute name="group" description="The ApplicationQuestionGroup for this section" type="ApplicationQuestionGroup" required="true"/>
    <apex:attribute name="preventUpdates" description="" type="Boolean" default="false" required="false"/>
    <apex:attribute name="appControl" description="ApplicationSettings" type="Application_Control__c" required="true"/>
    <apex:attribute name="groupNewRelatedRecord" description="" type="ApexPages.Action" required="true"/>
    <apex:attribute name="obj" description="" type="sObject" required="false"/>
    <apex:variable value="{!1}" var="placeholderUUID"/>
    <div xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <apex:outputPanel styleClass="{!if(group.isEditable,'','fieldNotEditable')}" id="theSection" layout="block">
            <!--<div class="progress">
                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40"
                     aria-valuemin="0" aria-valuemax="100" style="width:{!group.progress}%;background-color:#{!appControl.Arrow_Color__c};">
                </div>
            </div>-->

            <apex:outputPanel layout="none" rendered="{!group.instructions != null && group.instructions != ''}">
                <div class="slds-text-body_regular">
                    <p>
                        <apex:outputText value="{!group.instructions}" escape="false"/>
                    </p>
                </div>
            </apex:outputPanel>

            <apex:outputPanel layout="none" rendered="{!group.isInternal && (group.response != null && group.response.Fulfilled_On__c != null)}">
                <p>
                    <apex:outputText value="Completed on {0, Date}.">
                        <apex:param value="{!group.response.Fulfilled_On__c}"/>
                    </apex:outputText>
                </p>
                <br/>
            </apex:outputPanel>

            <apex:outputPanel layout="none" rendered="{!group.isInternal && (group.response == null || group.response.Fulfilled_On__c == null)}">
                <p>This has not been completed.</p>
                <br/>
            </apex:outputPanel>

            <apex:outputPanel id="questionGroupWrapper" layout="block">
                <apex:outputPanel id="questionGroup" rendered="{!group.questionGroup && !group.isInternal}" layout="block">

                    <apex:actionFunction name="sendTheReminder{!group.item.Id}" action="{!group.sendReminder}" reRender="none"/>
                    <apex:actionRegion >

                        <apex:repeat value="{!group.questions}" var="question">
                            <apex:variable value="{!placeholderUUID+1}" var="placeholderUUID"/>
                            <apex:variable value="{!IF(question.question.Id == null, 'label' + TEXT(placeholderUUID), question.question.id)}" var="qId"/>

                            <script>
                                placeholders["{!qId}"] = "{!IF(NOT(question.placeholder == null), question.placeholder, '' )}";
                                maxLengths["{!qId}"] = "{!if(question.length == null || question.length > 255, 255, question.length)}";
                            </script>
                            <div class="slds-p-vertical_xx-small slds-col slds-size_1-of-1 slds-large-size_12-of-12">
                                <apex:outputPanel rendered="{!question.showQuestion}" layout="none">

                                    <!-- SLDS Address -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Address'}" layout="none">
                                        HERE -1
                                        <div class="slds-form-element">
                                            <apex:outputLabel for="street" styleClass="slds-form-element__label">
                                                {!question.label} Street
                                                <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                    <abbr class='slds-required' title='required'>* </abbr>
                                                </apex:outputPanel>
                                            </apex:outputLabel>
                                            <div class="slds-form-element__control">
                                                <apex:inputField id="street" value="{!question.response.Street__c}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                            </div>
                                            <div class="slds-form-element__help" id="error-message-{!qId}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </div>
                                        </div>

                                        <div class="slds-form-element slds-p-vertical_xx-small">
                                            <apex:outputLabel for="city" styleClass="slds-form-element__label">
                                                {!question.label} City
                                                <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                    <abbr class='slds-required' title='required'>* </abbr>
                                                </apex:outputPanel>
                                            </apex:outputLabel>
                                            <div class="slds-form-element__control">
                                                <apex:inputField id="city" value="{!question.response.City__c}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                            </div>
                                            <div class="slds-form-element__help" id="error-message-{!qId}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </div>
                                        </div>

                                        <apex:outputPanel rendered="{!!question.stateAndCountryPicklistEnabled}" layout="none">

                                            <div class="slds-form-element slds-p-vertical_xx-small">
                                                <apex:outputLabel for="state" styleClass="slds-form-element__label">
                                                    {!question.label} State/Province
                                                    <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                        <abbr class='slds-required' title='required'>* </abbr>
                                                    </apex:outputPanel>
                                                </apex:outputLabel>
                                                <div class="slds-form-element__control">
                                                    <apex:inputField id="state" value="{!question.con[IF(question.stateAndCountryPicklistEnabled,'MailingStateCode','MailingState')]}" styleClass="slds-input {!qId}  {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                                </div>
                                                <div class="slds-form-element__help" id="error-message-{!qId}">
                                                    <apex:outputText value="{!question.helpText}" escape="false"/>
                                                </div>
                                            </div>

                                            <div class="slds-form-element slds-p-vertical_xx-small">
                                                <apex:outputLabel for="country1" styleClass="slds-form-element__label">
                                                    {!question.label} Country
                                                    <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                        <abbr class='slds-required' title='required'>* </abbr>
                                                    </apex:outputPanel>
                                                </apex:outputLabel>
                                                <div class="slds-form-element__control">
                                                    <apex:inputField id="country1" value="{!question.con[IF(question.stateAndCountryPicklistEnabled,'MailingCountryCode','MailingCountry')]}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                                </div>
                                                <div class="slds-form-element__help" id="error-message-{!qId}">
                                                    <apex:outputText value="{!question.helpText}" escape="false"/>
                                                </div>
                                            </div>

                                        </apex:outputPanel>

                                        <apex:outputPanel rendered="{!question.stateAndCountryPicklistEnabled}" layout="none">

                                            <div class="slds-form-element slds-p-vertical_xx-small">
                                                <apex:outputLabel for="country" styleClass="slds-form-element__label">
                                                    {!question.label} Country
                                                    <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                        <abbr class='slds-required' title='required'>* </abbr>
                                                    </apex:outputPanel>
                                                </apex:outputLabel>
                                                <div class="slds-form-element__control">
                                                    <div class="slds-select_container">
                                                        <apex:inputField id="country" value="{!question.con[IF(question.stateAndCountryPicklistEnabled,'MailingCountryCode','MailingCountry')]}" styleClass="slds-select {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                                    </div>
                                                </div>
                                                <div class="slds-form-element__help" id="error-message-{!qId}">
                                                    <apex:outputText value="{!question.helpText}" escape="false"/>
                                                </div>
                                            </div>

                                            <div class="slds-form-element dependent-picklist slds-p-vertical_xx-small">
                                                <apex:outputLabel for="statePicklist" styleClass="slds-form-element__label">
                                                    {!question.label} State/Province
                                                    <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                        <abbr class='slds-required' title='required'>* </abbr>
                                                    </apex:outputPanel>
                                                </apex:outputLabel>
                                                <div class="slds-form-element__control">
                                                    <div class="slds-select_container ">
                                                        <apex:inputField id="statePicklist" value="{!question.con[IF(question.stateAndCountryPicklistEnabled,'MailingStateCode','MailingState')]}" styleClass="slds-select {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                                    </div>
                                                </div>
                                                <div class="slds-form-element__help" id="error-message-{!qId}">
                                                    <apex:outputText value="{!question.helpText}" escape="false"/>
                                                </div>
                                            </div>

                                        </apex:outputPanel>

                                        <div class="slds-form-element slds-p-vertical_xx-small">
                                            <apex:outputLabel for="postal" styleClass="slds-form-element__label">
                                                {!question.label} Postal Code
                                                <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                    <abbr class='slds-required' title='required'>* </abbr>
                                                </apex:outputPanel>
                                            </apex:outputLabel>
                                            <div class="slds-form-element__control">
                                                <apex:inputField id="postal" value="{!question.response.PostalCode__c}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                            </div>
                                            <div class="slds-form-element__help" id="error-message-{!qId}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </div>
                                        </div>

                                    </apex:outputPanel>
                                    <!-- End Address -->

                                    <apex:outputPanel rendered="{!question.fieldType == 'Checkbox'}" layout="none">
                                        HERE 1
                                        <div class="checkbox">
                                            <label for="field">
                                                <apex:inputCheckbox id="checkBoxField" onchange="reRenderAllGroups();pageLoadReRendered();" value="{!question.response.ValueCheckbox__c}" styleClass="{!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                                {!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}
                                            </label>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>


                                    <apex:outputPanel rendered="{!question.fieldType == 'Currency'}" layout="none">
                                        HERE 2
                                        <div class="form-group">
                                            <label for="CurrencyField">{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><em class="glyphicon glyphicon-usd"></em></span>
                                                <apex:inputField id="CurrencyField" value="{!question.response.ValueCurrency__c}" styleClass="form-control {!question.question.Id} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}" style="font-family:'Open Sans', sans-serif;color:#{!appControl.Text_Color__c};font-weight:400;font-size:14px;height:36px;" rendered="{!question.response != null}"/>
                                            </div>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Date -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Date'}" layout="block" styleClass="slds-form-element">
                                        <label class="slds-form-element__label" for="DateField">
                                            <apex:outputPanel rendered="{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),true,false)}">
                                                <abbr class="slds-required" title="required">* </abbr>
                                            </apex:outputPanel>
                                            {!question.label}
                                        </label>
                                        <div class="slds-form-element__control">
                                            <div aria-describedby="error-{!qId}" aria-invalid="true">
                                                <apex:input id="DateField" type="date" value="{!question.dateValue}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                            </div>
                                        </div>
                                        <div class="slds-form-element__help" id="error-{!qId}">
                                            <apex:outputText value="{!question.helpText}" escape="false"/>
                                        </div>
                                    </apex:outputPanel>


                                    <apex:outputPanel rendered="{!question.fieldType == 'DateTime'}" layout="none">
                                        HERE 3
                                        <div class="form-group">
                                            <label for="dateTimeField">{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><em class="glyphicon glyphicon-time"></em></span>
                                                <apex:input id="dateTimeField" type="auto" value="{!question.datetimeValue}" styleClass="form-control {!question.question.Id} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}" style="font-family:'Open Sans', sans-serif;color:#{!appControl.Text_Color__c};font-weight:400;font-size:14px;height:36px;"/>
                                            </div>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>


                                    <apex:outputPanel rendered="{!question.fieldType == 'Decimal' || question.fieldType == 'Integer' || question.fieldType == 'Number'}" layout="none">
                                        HERE 4
                                        <div class="form-group">
                                            <label for="numberField">{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}</label>
                                            <apex:inputField id="numberField" value="{!question.response.ValueNumber__c}" styleClass="form-control {!question.question.Id} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}" style="font-family:'Open Sans', sans-serif;color:#{!appControl.Text_Color__c};font-weight:400;font-size:14px;height:36px;"/>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Email -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Email'}" layout="block" styleClass="slds-form-element">
                                        <apex:outputLabel for="field" styleClass="slds-form-element__label">
                                            {!question.label}
                                            <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                <abbr class='slds-required' title='required'>* </abbr>
                                            </apex:outputPanel>
                                        </apex:outputLabel>
                                        <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                            <svg class="slds-icon slds-input__icon slds-input__icon_right slds-icon-text-default" aria-hidden="true">
                                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/apexpages/slds/latest/assets/icons/utility-sprite/svg/symbols.svg#email"></use>
                                            </svg>
                                            <apex:inputField id="field" type="email"
                                                             styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"
                                                             value="{!question.response.ValueText__c}"
                                                             html-aria-invalid="true"
                                            />
                                            <apex:commandLink styleClass="btn-lightning" style="width:1%;display:table-cell;color:#{!appControl.Button_Text_Color__c};background-color:#{!appControl.Button_Color__c};" value="Send Reminder" onclick="appShowLoadingSpinner();sendTheReminder{!group.item.Id}();" oncomplete="appHideLoadingSpinner();pageLoadReRendered();" reRender="none" rendered="{!!preventUpdates && question.isEditable && question.question.External_Email_Field__c == true}"/>
                                        </div>
                                        <div class="slds-form-element__help" id="error-message-{!qId}">
                                            <apex:outputText value="{!question.helpText}" escape="false"/>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Password Encrypted Text Input -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'TextEncrypted'}" layout="block" styleClass="slds-form-element">
                                        <apex:outputLabel styleClass="lds-form-element__label" for="textEncryptedField">
                                            {!question.label}
                                            <apex:outputPanel layout="none" rendered="{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),true,false)}">
                                                <abbr class="slds-required" title="required">* </abbr>
                                            </apex:outputPanel>
                                        </apex:outputLabel>
                                        <div class="slds-form-element__control">
                                            <div aria-describedby="error-{!qId}">
                                                <apex:inputSecret redisplay="true" id="textEncryptedField" value="{!question.textEncryptedValue}" styleClass="slds-input {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"/>
                                            </div>
                                        </div>
                                        <div class="slds-form-element__help" id="error-{!qId}">
                                            <apex:outputText value="{!question.helpText}" escape="false"/>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Mulit Picklist -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'MultiPicklist'}" layout="block" styleClass="slds-form-element">
                                        <apex:outputLabel for="field" styleClass="slds-form-element__label">
                                            {!question.label}
                                            <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                <abbr class='slds-required' title='required'>* </abbr>
                                            </apex:outputPanel>
                                        </apex:outputLabel>
                                        <div class="slds-form-element__control">
                                            <div class="slds-checkbox">
                                                <apex:selectCheckboxes id="multiPicklistField" onchange="reRenderAllGroups();pageLoadReRendered();" value="{!question.multiPicklistValues}" layout="pageDirection" styleClass="slds-tabled-checklist">
                                                    <apex:selectOptions value="{!question.options}"/>
                                                </apex:selectCheckboxes>
                                                <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                    <apex:outputText value="{!question.helpText}" escape="false"/>
                                                </p>
                                            </div>
                                        </div>
                                    </apex:outputPanel>


                                    <apex:outputPanel rendered="{!question.fieldType == 'Percent'}" layout="none">
                                        HERE 5
                                        <div class="form-group">
                                            <label for="percentField">{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}</label>
                                            <div class="input-group">
                                                <span class="input-group-addon">%</span>
                                                <apex:inputField id="percentField" value="{!question.response.ValuePercent__c}" styleClass="form-control {!question.question.Id} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}" style="font-family:'Open Sans', sans-serif;color:#{!appControl.Text_Color__c};font-weight:400;font-size:14px;height:36px;"/>
                                            </div>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Phone -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Phone'}" layout="none">
                                        <apex:outputLabel for="phoneField" styleClass="slds-form-element__label">
                                            {!question.label}
                                            <apex:outputPanel layout="none" rendered="{!CONTAINS(question.question.Required__c,question.application.Application_Status__c)}">
                                                <abbr class='slds-required' title='required'>* </abbr>
                                            </apex:outputPanel>
                                        </apex:outputLabel>
                                        <apex:variable value="{!question.placeholder}" var="placeholder"/>
                                        <div class="slds-form-element__control">
                                            <apex:inputField id="phoneField"
                                                             type="tel"
                                                             styleClass="slds-input {!qId} validPhone {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}"
                                                             value="{!question.response.ValuePhone__c}"
                                                             html-aria-invalid="true"
                                            />
                                        </div>
                                        <div class="slds-form-element__help" id="error-message-{!qId}">
                                            <apex:outputText value="{!question.helpText}" escape="false"/>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS picklist -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Picklist' && !question.radio}" layout="block" styleClass="slds-form-element">
                                        <apex:outputLabel styleClass="slds-form-element__label" for="picklistField">
                                            {!question.label}
                                            <apex:outputPanel layout="none" rendered="{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),true,false)}">
                                                <abbr class="slds-required" title="required">* </abbr>
                                            </apex:outputPanel>
                                        </apex:outputLabel>
                                        <div class="slds-form-element__control">
                                            <div class="slds-select_container">
                                                <div aria-describedby="error-{!qId}">
                                                    <apex:selectList id="picklistField" onchange="reRenderAllGroups();pageLoadReRendered();" value="{!question.response.ValueText__c}" size="1" styleClass="slds-select {!qId} {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}">
                                                        <apex:selectOptions value="{!question.options}"/>
                                                    </apex:selectList>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="slds-form-element__help" id="error-{!qId}">{!question.helpText}</div>
                                    </apex:outputPanel>


                                    <apex:outputPanel rendered="{!(question.fieldType == 'Picklist' && question.radio) || question.fieldType == 'Radio'}" layout="none">
                                        HERE 7
                                        <div class="form-group">
                                            HERE 1 <label for="radioField">{!question.label}{!if(CONTAINS(question.question.Required__c,question.application.Application_Status__c),'*','')}</label>
                                            <apex:selectRadio id="radioField" onchange="reRenderAllGroups();pageLoadReRendered();" value="{!question.response.ValueText__c}" layout="pageDirection" styleClass="selectRadio {!if(question.isEditable && group.isEditable && !preventUpdates,'','fieldNotEditable')}" style="font-family:'Open Sans', sans-serif;color:#{!appControl.Text_Color__c};font-weight:400;font-size:14px;height:36px;">
                                                <apex:selectOptions value="{!question.options}"/>
                                            </apex:selectRadio>
                                            <p class="help-block" style="{!IF(question.helpText != null && !ISBLANK(question.helpText), '', 'display:none;')}">
                                                <apex:outputText value="{!question.helpText}" escape="false"/>
                                            </p>
                                        </div>
                                    </apex:outputPanel>

                                    <!-- SLDS Reference Picklist -->
                                    <apex:outputPanel rendered="{!question.fieldType == 'Reference'}" layout="block" styleClass="slds-form-element">
